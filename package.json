{
  "name": "fjp",
  "version": "0.1.0",
  "description": "functional javascript playground",
  "module": "es/index.js",
  "main": "cjs/index.js",
  "unpkg": "dist/fjp.min.js",
  "jsdelivr": "dist/fjp.min.js",
  "browser": "dist/fjp.min.js",
  "files": [
    "es",
    "src",
    "dist"
  ],
  "scripts": {
    "test:bench": "npm-run-all test:cov bench",
    "test:changed-silent": "cross-env BABEL_ENV=cjs NODE_ENV=test jest -o --passWithNoTests --silent",
    "test:cov": "cross-env BABEL_ENV=cjs NODE_ENV=test jest --verbose --passWithNoTests --coverage",
    "test": "cross-env BABEL_ENV=cjs NODE_ENV=test jest --verbose --passWithNoTests",
    "bench": "cross-env BABEL_ENV=cjs node ./benchRunner.js",
    "build:es": "cross-env BABEL_ENV=es babel source --out-dir es --ignore **/*.test.js,**/*.bench.js",
    "build:cjs": "cross-env BABEL_ENV=cjs babel source --out-dir cjs --ignore **/*.test.js,**/*.bench.js",
    "build:umd": "cross-env NODE_ENV=development BABEL_ENV=umd rollup -c -o dist/fjp.js",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=umd rollup -c -o dist/fjp.min.js",
    "build": "npm-run-all test clean build:*",
    "readme": "node doc/docs.js",
    "prettier": "prettier --config .prettierrc.js --write \"**/*.{js,ts,md}\"",
    "lint": "eslint -c .eslintrc.js --fix --color --ignore-path .eslintignore \"**/*.{js,ts}\"",
    "clean": "npm-run-all prettier lint",
    "release:first": "npm run build && npm run readme && standard-version --first-release",
    "release": "npm run build && npm run readme && standard-version --no-verify",
    "release:full": "npm run release && git push --follow-tags origin master && npm publish"
  },
  "jest": {
    "modulePaths": [
      "source"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "/dist/"
    ],
    "testEnvironment": "jsdom"
  },
  "author": "bryan@bryanckizer.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belsrc/fjp.git"
  },
  "bugs": {
    "url": "https://github.com/belsrc/fjp/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/plugin-external-helpers": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/register": "7.17.7",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-node-resolve": "13.2.1",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "benchmark": "2.1.4",
    "cli-table": "0.3.11",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-belsrc": "1.1.2",
    "glob": "8.0.1",
    "jest": "27.5.1",
    "jsdoc-to-markdown": "7.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rollup": "2.70.2",
    "rollup-plugin-terser": "7.0.2",
    "standard-version": "9.3.2"
  },
  "dependencies": {}
}
